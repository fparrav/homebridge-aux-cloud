{
  "name": "homebridge-aux-cloud",
  "version": "0.0.2-beta.5",
  "description": "Homebridge plugin for AUX Cloud appliances (ported from maeek/ha-aux-cloud).",
  "license": "MIT",
  "keywords": [
    "homebridge-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fparrav/homebridge-aux-cloud.git"
  },
  "bugs": {
    "url": "https://github.com/fparrav/homebridge-aux-cloud/issues"
  },
  "homepage": "https://github.com/fparrav/homebridge-aux-cloud#readme",
  "engines": {
    "homebridge": ">=1.6.0",
    "node": ">=18.15.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "config.schema.json"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "watch": "tsc -p tsconfig.build.json --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "echo \"No automated tests yet\""
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "homebridge": "^1.7.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^25.0.1",
    "typescript": "~5.3.3"
  }
}
